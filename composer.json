{
    "name": "kenny1911/project-tempalte",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix",
        "cs-check": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer check",
        "phpunit": "phpunit",
        "psalm":"./tools/psalm/vendor/bin/psalm",
        "checks": [
            "@cs-check",
            "@phpunit",
            "@psalm -- --no-cache"
        ]
    },
    "autoload": {
        "psr-4": {
            "Kenny1911\\ProjectName\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kenny1911\\Tests\\ProjectName\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4"
    }
}
